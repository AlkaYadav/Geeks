struct SumPair{
	int sum;
	int first;
	int second;
};

void findFourPairSum(int a[],int n){
	struct SumPair sumPair[n*(n-1)/2];
	int indx=0;
	for(int i=0;i<n-1;i++){
		for(int j=i+1;j<n;j++){
			sumPair[indx++]=a[i]+a[j];
		}
	}

