int findPivot(int arr[],int start,int end){
	int mid=(start+end)/2;
	if(start>end){
		return -1;
	}
	if(arr[mid]>arr[mid+1])
}
void sortedPivotedSearch(int arr[],int n){
	int pivot=findPivot(arr,0,n-1);
}


