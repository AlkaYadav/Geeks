#include<iostream>
using namespace std;
void LCS(int X[],int Y[],imt m,int n){
	int LCS[m][n];

	//Initializing table
	for(int i=0;i<m;i++){
		for(int j=0;j<n;j++){
			LCS[i][0]=0;
			LCS[0][j]=0;
		}
	}

	//Fill the table
	for(int i=1;i<m;i++){
		for(int j=1;j<n;j++){
			if(X[i]==Y[j]){
				LCS[i][j]=1+LCS[i-1][j-1];
			}
			else{
				LCS[i][j]=max(LCS[i-1][j],LCS[i][j-1]);
			}
		}
	}

	//Legth
}



