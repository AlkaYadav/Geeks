//i=1,j=length
int longestPalindromicSubseq(char X[],int i,int j)
{
	if(j<=0 || i>j-1){
		return 0;
	}
	if(X[i-1]==Y[j-1]){
		return 2+longestPalindromicSubseq(X,i+1,j-1);
	}
	else return max(longestPalindromicSubseq(X,i,j-1),longestPalindromicSubseq(X,i+1,j));
}


