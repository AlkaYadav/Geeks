#include<string>
#include<map>
using namespace std;

void findItinary(char **str1,char** str2,int len){
	//Create map of two strings
	map<string,string> mapHash;
	for(int i=0;i<len;i++){
		mapHash[str1[i]]=str2[i];
	}

	//ReverseMap
	map<string,string> revmapHash;
	for(int i=0;i<len;i++){
			mapHash[str2[i]]=str1[i];
		}
}


