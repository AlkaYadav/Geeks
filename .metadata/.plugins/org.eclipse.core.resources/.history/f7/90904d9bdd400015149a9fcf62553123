/*
 * MaximumIncreasingSumSubseq.cpp
 *
 *  Created on: Aug 12, 2015
 *      Author: user
 */

void maximumIncreasingSumSubsequence(int arr[],int n){
	int maxlis[n+1];
	for(int i=0;i<n;i++){
		maxlis[i]=arr[i];
	}
	for(int i=1;i<n;i++){
		for(int j=1;j<i;j++){
			if(arr[j]<arr[i] && maxlis[i]<maxlis[j]+1){
				maxlis[i]<maxlis[j]+1;
			}
		}
	}

	cout<<"MaximumIncreasingSumSubseq length is "<<maxlis[n]<<endl;
}



