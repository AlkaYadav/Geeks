//Largest subarray with equal number of 0s and 1s

void largestSubArray(int a[0],int n){
	for(int i=0;i<n;i++){
		if(a[i]==0){
			a[i]=-1;
		}
	}

	int sum=0;
	for(int i=1;i<n;i++){
		sum+=a[i];
		sumLeft[i]=sum;
	}
}



