#include<iostream>
#include<limits.h>
#include<algorithm>
#include<vector>
#include<stack>
#include<map>
#include<functional>
using namespace std;

void nextGreaterEle(int a[],int n){
	stack<int> s;
	s.push(a[0]);
	for(int i=1;i<n;i++){
		int next=a[i];
		if(!s.empty()){
			int ele=s.pop();
			while(ele<next){
				cout<<ele<<" "<<next<<endl;
				if(!s.empty()){
					ele=s.top();
					s.pop();
				}
				else break;
			}
		}
	}
}

