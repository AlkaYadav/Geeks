#include<iostream>
#include<limits.h>
#include<map>
#include<cstring>
#include<algorithm>
using namespace std;
int compare(const void* a,const void * b){
	return *(int*)a-*(int*)b;
}
struct SumPair{
	int sum;
	int first;
	int second;
};

void findFourPairSum(int a[],int n){
	struct SumPair sumPair[n*(n-1)/2];
	int indx=0;
	for(int i=0;i<n-1;i++){
		for(int j=i+1;j<n;j++){
			sumPair[indx++]=a[i]+a[j];
		}
	}
	qsort(sumPair,n*(n-1)/2,sizeof(sumPair[0]),compare);
	int indx=0;
	int k=0;
	int l=n-1;
	while(k<l){
		if(sumPair)
	}
}


