#include<queue>
#include <iostream>
#include<stdlib.h>
#include<vector>
using namespace std;

struct Edge{
	int dest;
	int cost;
	int dist;
};


int main(){
		int N,M,src,wt;
		int dest;
			int cost;
			int dist;
		int sum=0;
		cin>>N>>M;
		vector<struct Edge*> v[N+1];
		for(int i=0;i<M;i++){
			cin>>dest;
			cin>>cost;
			cin>>dist;
			struct Edge *e=(struct Edge *)malloc(sizeof(struct Edge));
			e->dest=dest;
			e->cost=cost;
			e->dist=dist;
			v[i].push_back(e);
		}
		dfs(v,N)
		vector<struct Edge*>::iterator it;
		int i=0;
		for(it=v[i].begin();it!=v[i].end();it++){
			struct Edge* p=*it;
			cout<<"dest is "<<p->dest<<endl;
		}

}
