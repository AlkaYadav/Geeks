//Maximum Sum Path in Two Arrays
void maxSumPath(int a1[],int a2[],int s1,int s2){
	int i=0,j=0;
	int sum1=0,sum2=0,sum=0;
	while(i<s1&& j<s2){
		if(a1[i]==a2[i]){
			sum+=a1[i];

			i++;
			j++;
		}
		else if(a1[i]<a2[i]){
			sum1+=a1[i];
			i++;
			sum=max(sum1,sum2);
		}
		else{
			sum2+=a2[j];
			j++;
			sum=max(sum1,sum2);
		}


	}
}



