// Description : Longest Increasing Subsequence
//============================================================================

#include <iostream>
#include<limits.h>
using namespace std;

void longestIncreaingSubsequence(int arr[],int n){
	int lis[n+1];
	int max_lis=INT_MIN;
	for(int i=1;i<=n;i++){
		lis[i]=1;
	for(int i=1;i<=n;i++){
		for(int j=1;j<=i;j++){
			if(arr[j]<arr[i]&& lis[i]<lis[j]+1){
				lis[i]=1+lis[j];
			}
		}
	}

	for(int k=1;k<=n;k++){
		if(lis[k]>max_lis){
			max_lis=lis[k];
		}
	}
	return max_lis;
	}
}
int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
