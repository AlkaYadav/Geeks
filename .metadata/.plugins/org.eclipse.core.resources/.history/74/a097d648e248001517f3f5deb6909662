//Implement k stacks in an array

class kStacks{
	int *arr;
	int *top;
	int *next;
	int free;
public:
	kStacks(int arr[],int k,int n);
	void push(int sn,int item);
	int pop(int sn);
};

kStacks::kStacks(int a[],int k,int n){
	top=new int[k];
	next=new int[n];
	arr=new int[n];
	free=0;
	for(int i=0;i<k;i++){
		top[i]=-1;
	}
	for(int i=0;i<n-1;i++){
		next[i]=i+1;
	}
	next[n-1]=-1;
}

void kStacks::push(int sn,int item){
	int i=free;
	free=next[free];
	if(top[sn]==-1){
		top[sn]=
	}
}

