#include <iostream>
using namespace std;

void printStair(int h){
	for(int i=1;i<=h;i++){
		for(int j=h-i;j>0;j++){
			cout<<"";
		}
		for(int k=i;k>0;k++){
			cout<<"#";
		}
		cout<<endl;
	}
	return;
}
int main() {
	cout<<"alka";
	printStair(5);
	return 0;
}
