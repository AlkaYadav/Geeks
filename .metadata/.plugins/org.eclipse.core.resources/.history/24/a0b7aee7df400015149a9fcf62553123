/*
 * LongestBitonicSubsequence.cpp
 *
 *  Created on: Aug 12, 2015
 *      Author: user
 */

void longestBitonicSubsequence(int arr[],int n){
	int lis[n];
	int lds[n];
	for(int i=0;i<n;i++){
		lis[i]=1;
		lds[i]=1;
	}

	//Computing Longest Increasing Subsequece
	for(int i=1;i<n;i++){
		for(int j=0;j<i;j++){
			if(arr[j]<arr[i]&& lis[j]+1>lis[i]){
				lis[i]=lis[j]+1;
			}
		}
	}
}



