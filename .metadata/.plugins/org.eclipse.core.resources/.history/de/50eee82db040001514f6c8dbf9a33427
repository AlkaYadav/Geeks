#include<limits.h>
#include<iostream>
using namespace std
void matrixChainMultiply(int p[],int n){
	int m[n][n],cost;
	for(int i=0;i<n;i++){
		for(int j=0;j<n;j++){
			if(i==j){
				m[i][j]=0;
			}
			else{
				m[i][j]=INT_MAX;
				for(int k=i;k<=j;k++){
					cost=m[i-1][k]+m[k+1][j]+p[i-1]*p[k]*p[j];
					if(cost<m[i][j]){
						m[i][j]=cost;
					}
				}
			}
		}
	}
//Printing Matrix...
for(int i=0;i<n;i++){
		for(int j=0;j<n;j++){
			cout<<m[i][j]<<" ";
		}
		cout<<endl;
}
	cout<<"Minimum cost is "<<m[0][n-1]<<endl;
}


