int 01Knapsack_Recursive(int wt[],int val[],int n,int W){
	if(n<0){
		return 0;
	}
	if(wt[n-1]>W){
		return 01Knapsack_Recursive(wt,val,n-1,W);
	}
	else{
		return max(01Knapsack_Recursive(wt,val,n-1,W-wt[n-1]),01Knapsack_Recursive(wt,val,n-1,W));
	}
}


