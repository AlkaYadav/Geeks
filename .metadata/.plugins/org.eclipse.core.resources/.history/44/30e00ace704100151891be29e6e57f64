#include<iostream>
#include<cstring>
using namespace std;

int LCS_Recursive(char X[],char Y[],int m,int n){
	if(m<=0|| n<=0){
		return 0;
	}
	if(X[m-1]==Y[n-1]){
		return 1+LCS_Recursive(X,Y,m-1,n-1);
	}
	else{
		return max(LCS_Recursive(X,Y,m-1,n),LCS_Recursive(X,Y,n-1,m));
	}
}


int main(){
	char X[]="ABCDGH";
	char Y[]="AEDFHR";
	int m=strlen(X);
	int n=strlen(Y);

}
