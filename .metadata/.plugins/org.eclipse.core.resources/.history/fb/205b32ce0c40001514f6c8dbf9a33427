// Description : Longest Increasing Subsequence
//============================================================================

#include <iostream>
using namespace std;

void longestIncreaingSubsequence(int arr[],int n){
	int lis[n+1];
	for(int i=1;i<=n;i++){
		lis[i]=1;
	for(int i=1;i<=n;i++){
		for(int j=1;j<=i;j++){
			if(arr[j]<arr[i]&& j<i){
				lis[i]=1+lis[j];
			}
		}
	}
	}
}
int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
