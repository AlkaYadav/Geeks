#include<queue>
#include <iostream>
#include<vector>
using namespace std;

void dfs(vector<pair<int,int> > *v,int N,int &sum){

}
int main(){
	int N,M,src,dest,wt;
	int sum=0;
	cin>>N>>M;
	vector<pair<int,int> > v[N];
	for(int i=0;i<M;i++){
			cout<<"Enter "<<(i+1)<<"edge"<<endl;
			cin>>src>>dest>>wt;
			v[src].push_back(make_pair(dest,wt));
	}

	dfs(v,N,sum);

}

