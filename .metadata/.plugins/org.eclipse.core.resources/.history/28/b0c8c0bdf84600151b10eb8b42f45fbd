#include<queue>
#include<string.h>
using namespace std;
//An Interesting Method to Generate Binary Numbers from 1 to n

void generateBinaryNumbers(int n){
	queue<string> q;
	q.push('1');
	while(n--){
		string s1=q.pop();
		cout<<s1<<endl;

	}
}


