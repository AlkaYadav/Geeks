#include<iostream>
using namespace std;

void binomialCoefficient(int n,int k){

}
void binomialCoefficient(int n,int k){
	int binomial[n+1][k+1];
	int i,j;
	for(i=0;i<=n;i++){
		for(j=i;j<=min(i,k);j++){
			if(i==j||j==0){
				binomial[i][j]=1;
			}
			else{
			binomial[i][j]=binomial[i-1][j-1]+binomial[i-1][j];
		}
	}
}
cout<<"Binomial is "<<binomial[n][k]<<endl;
}

int main(){
	cout<<"Binomial Coefficient is "<<endl;
			binomialCoefficient(5,2);
}
