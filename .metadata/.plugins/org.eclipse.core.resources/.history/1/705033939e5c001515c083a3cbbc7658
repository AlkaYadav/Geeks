#include<queue>
#include <iostream>
using namespace std;
void bfsUtil(vector<int> *v,bool *visited,int i,queue<int> q){
	visited[i]=true;
	for()
}
void bfs(vector<int> *v){
	bool *visited=new bool[v->size()+1];
	for(int i=1;i<v->size();i++){
		visited[i]=false;
	}
	for(int i=1;i<v->size();i++){
		if(visited[i]==false){
			bfsUtil(v,visited,i,q);
		}
	}
}
int main(){
	int N,M,src,dest;
	cin>>N>>M;
	vector<int> v[N+1];
	cout<<"Vector size "<<v->size();
	for(int i=0;i<M;i++){
		cin>>src>>dest;
		v[src].push_back(dest);
	}
	bfs(v);
}


